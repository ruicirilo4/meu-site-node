{
  "version": 2,
  "builds": [
    { "src": "/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/now-playing", "dest": "/index.js" },
    { "src": "/api/recent-songs", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 60
    }
  }
}

