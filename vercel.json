{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/now-playing",
      "dest": "/now-playing"
    },
    {
      "src": "/recent-songs",
      "dest": "/recent-songs"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
